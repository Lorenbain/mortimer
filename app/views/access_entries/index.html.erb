<h1>Access Log</h1>
 

<div id="content_body">
<h2>for <a href="<%= entry_url(@entry)%>"><%= @entry.title %></a></h2>
<table id="access_entry_tbl" class="common">
  <tr>
    <th>Access Type</th>
    <th>Login</th>
	<th>Date/Time</th>
  </tr>

<% for access_entry in @access_entries %>
  <tr>
    <td><%=h access_entry.accesstype %></td>
    <td><%=h access_entry.login %></td>
	<td><%=h access_entry.created_at %></td>
  </tr>
<% end %>
</table>
</div>

